Host *
    AddKeysToAgent yes
    IdentityFile ~/.ssh/id_ed25519

Host github.com
    User git
    Hostname github.com
    PreferredAuthentications publickey
    AddKeysToAgent yes
    IdentityFile ~/.ssh/id_ed25519

